{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "sandeepadf2025"
		},
		"AzureBlob_LS_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureBlob_LS'"
		},
		"adls_linkservice_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'adls_linkservice'"
		},
		"sql_db_linkservice_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'sql_db_linkservice'"
		},
		"AzureKeyVault1_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://sandeep28keyvault-1.vault.azure.net/"
		},
		"adls_linkservice_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://sandeep28stg1.dfs.core.windows.net/"
		},
		"sql_db_linkservice_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "sandeep28dbserver.database.windows.net"
		},
		"sql_db_linkservice_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "sandeepDB2025"
		},
		"sql_db_linkservice_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "sandeep28"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "sandeepadf2025_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureBlob_LS_connectionString": {
						"value": "[parameters('AzureBlob_LS_connectionString')]"
					},
					"adls_linkservice_accountKey": {
						"value": "[parameters('adls_linkservice_accountKey')]"
					},
					"sql_db_linkservice_password": {
						"value": "[parameters('sql_db_linkservice_password')]"
					},
					"AzureKeyVault1_properties_typeProperties_baseUrl": {
						"value": "[parameters('AzureKeyVault1_properties_typeProperties_baseUrl')]"
					},
					"adls_linkservice_properties_typeProperties_url": {
						"value": "[parameters('adls_linkservice_properties_typeProperties_url')]"
					},
					"sql_db_linkservice_properties_typeProperties_server": {
						"value": "[parameters('sql_db_linkservice_properties_typeProperties_server')]"
					},
					"sql_db_linkservice_properties_typeProperties_database": {
						"value": "[parameters('sql_db_linkservice_properties_typeProperties_database')]"
					},
					"sql_db_linkservice_properties_typeProperties_userName": {
						"value": "[parameters('sql_db_linkservice_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "sandeepadf2025_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'sandeepadf2025_ArmTemplate_0')]"
			]
		},
		{
			"name": "sandeepadf2025_ArmTemplate_2",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_2.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'sandeepadf2025_ArmTemplate_1')]"
			]
		},
		{
			"name": "sandeepadf2025_ArmTemplate_3",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_3.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'sandeepadf2025_ArmTemplate_2')]"
			]
		}
	]
}