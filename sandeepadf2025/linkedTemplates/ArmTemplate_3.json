		{
			"name": "[concat(parameters('factoryName'), '/Hashing DataFlow')]",
			"type": "Microsoft.DataFactory/factories/dataflows",
			"apiVersion": "2018-06-01",
			"properties": {
				"type": "MappingDataFlow",
				"typeProperties": {
					"sources": [
						{
							"dataset": {
								"referenceName": "SQL_DS_Source",
								"type": "DatasetReference"
							},
							"name": "Source"
						},
						{
							"dataset": {
								"referenceName": "Destination_DS",
								"type": "DatasetReference"
							},
							"name": "destination"
						}
					],
					"sinks": [
						{
							"dataset": {
								"referenceName": "Destination_DS",
								"type": "DatasetReference"
							},
							"name": "sink1"
						}
					],
					"transformations": [
						{
							"name": "derivedColumnSource"
						},
						{
							"name": "derivedColumnDestination"
						},
						{
							"name": "join1"
						},
						{
							"name": "filter1"
						},
						{
							"name": "select1"
						}
					],
					"scriptLines": [
						"source(output(",
						"          id as integer,",
						"          name as string,",
						"          city_name as string",
						"     ),",
						"     allowSchemaDrift: true,",
						"     validateSchema: false,",
						"     isolationLevel: 'READ_UNCOMMITTED',",
						"     format: 'table') ~> Source",
						"source(output(",
						"          id as integer,",
						"          name as string,",
						"          city_name as string",
						"     ),",
						"     allowSchemaDrift: true,",
						"     validateSchema: false,",
						"     isolationLevel: 'READ_UNCOMMITTED',",
						"     format: 'table') ~> destination",
						"Source derive(MD5Source = md5(concat(toString(id), name, city_name))) ~> derivedColumnSource",
						"destination derive(MD5Destination = md5(concat(toString(id), name, city_name))) ~> derivedColumnDestination",
						"derivedColumnSource, derivedColumnDestination join(MD5Source == MD5Destination,",
						"     joinType:'left',",
						"     matchType:'exact',",
						"     ignoreSpaces: false,",
						"     broadcast: 'auto')~> join1",
						"join1 filter(isNull(MD5Destination)) ~> filter1",
						"filter1 select(mapColumn(",
